{
  "name": "server",
  "version": "1.0.0",
  "main": "server.ts",
  "license": "MIT",
  "scripts": {
    "start": "ts-node server.ts",
    "dev": "nodemon server.ts",
    "generate-doc": "swagger-cli bundle docs/swagger.yaml --outfile docs/_build/swagger.yaml --type yaml",
    "dev:doc": " yarn generate-doc && yarn dev",
    "start:doc": "yarn generate-doc && ts-node server.ts",
    "make:model": "ts-node cli/index.ts generate:model",
    "make:service": "ts-node cli/index.ts generate:service",
    "make:controller": "ts-node cli/index.ts generate:controller",
    "make:route": "ts-node cli/index.ts generate:route",
    "make:all": "ts-node cli/index.ts generate:all",
    "make:all:schema": "ts-node cli/index.ts generate:all schema"
  },
  "engines": {
    "node": ">=16.20.1"
  },
  "bin": {
    "myCli": "./cli/index.ts"
  },
  "dependencies": {
    "@types/commander": "^2.12.2",
    "bcrypt": "^5.1.1",
    "cloudinary": "^1.40.0",
    "commander": "^11.0.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "swagger-cli": "^4.0.4",
    "swagger-ui-express": "^5.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "validator": "^13.11.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.5",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.5.9",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.11.1",
    "@types/yamljs": "^0.2.31",
    "morgan": "^1.10.0",
    "nodemon": "^3.0.1"
  }
}
